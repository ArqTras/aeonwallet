<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_transfer">
    <fragment
        android:id="@+id/navigation_transfer"
        android:name="com.aeon.app.ui.transfer.TransferFragment"
        android:label="Transfer"
        tools:layout="@layout/fragment_transfer" />
    <fragment
        android:id="@+id/navigation_recent"
        android:name="com.aeon.app.ui.recent.RecentFragment"
        android:label="Recents"
        tools:layout="@layout/layout_recent_list" />
    <fragment
        android:id="@+id/navigation_contact"
        android:name="com.aeon.app.ui.contact.ContactFragment"
        android:label="Contacts"
        tools:layout="@layout/fragment_contact" />
    <fragment
        android:id="@+id/navigation_wallet"
        android:name="com.aeon.app.ui.wallet.WalletFragment"
        android:label="Wallet"
        tools:layout="@layout/fragment_wallet" />
    <fragment
        android:id="@+id/navigation_node"
        android:name="com.aeon.app.ui.node.NodeFragment"
        android:label="Node"
        tools:layout="@layout/fragment_node" />
    <fragment
        android:id="@+id/navigation_new_wallet"
        android:name="com.aeon.app.NewWalletFragment"
        android:label="New Wallet"
        tools:layout="@layout/fragment_new_wallet" />
</navigation>